{% for flashMessage in app.session.flashbag.get('warning') %}
<section class="checkout-section" style="margin-top: 12px">
    <div class="container">
        <div class="alert alert-block alert-warning">
            <p>{{ flashMessage }}</p>
        </div>
    </div>
</section>
